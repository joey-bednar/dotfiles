#!/bin/bash

set -euo pipefail
IFS=$'\n\t'

pushd "$HOME/dotfiles/install/jobs" > /dev/null
source ./core
source ./update
source ./zsh
source ./fzf
source ./tmux
source ./nvim
source ./node
source ./docker
source ./python
source ./kitty
source ./i3
source ./git
source ./k8s
source ./firefox
source ./dotfiles
popd > /dev/null
